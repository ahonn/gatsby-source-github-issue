  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/ahonn/gatsby-source-github-issue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahonn/gatsby-source-github-issue"
  },
  "bugs": {
